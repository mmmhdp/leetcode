import re
class Solution:
    def isPalindrome(self, s: str) -> bool:

        s = re.sub('[^a-zA-Z0-9]+', '', s).lower()
        print(s)
        if len(s) == 1 or len(s) == 0:
            return True
        
        if len(s) == 2:
            return True if s[0] == s[1] else False
        
        for ind in range(len(s)//2):
            if s[ind] != s[len(s) - 1 - ind]:
                return False
        return True
